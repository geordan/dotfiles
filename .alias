alias "a"="ansible"
alias "ag"="ansible-galaxy"
alias "ap"="ansible-playbook"
alias "aws.config"="nvim ~/.aws/config"
alias "b"="bat --theme=gruvbox-dark"
alias "c"="clear"
alias "dcr"="docker-compose run"
alias "dcu"="docker-compose up"
alias "exa"="ls"
alias "le"="exa -la"
alias "t"="task"
alias "task-due-week"="task +WEEK list"
alias "in"='task add +in'
alias init.vim="nvim ~/.config/nvim/init.vim"
alias ohmyzsh="nvim ~/.oh-my-zsh"
alias rnd='task add +rnd +next'
alias rnr=read_and_review
alias think='tickle +1d'
alias tick=tickle
alias tkill="for s in \$(tmux list-sessions | awk '{print \$1}' | rg ':' -r '' | fzy); do tmux kill-session -t \$s; done;"
alias v="nvim"
alias vim="nvim"
alias vssh="vagrant ssh"
alias zsh.config="nvim ~/.zshrc"
