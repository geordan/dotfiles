alias a="ansible"
alias ag="ansible-galaxy"
alias ap="ansible-playbook"
alias aws.config="nvim ~/.aws/config"
alias b="bat --theme=gruvbox-dark"
alias bb="kubectl run busybox --image=busybox:1.28 --rm -it --restart=Never --command --"
alias c="clear"
alias dcr="docker-compose run"
alias dcu="docker-compose up"
alias exa="ls"
alias h="helm"
alias in='task add +in'
alias init.vim="nvim ~/.config/nvim/init.vim"
alias k="kubectl"
alias kaf="kubectl apply -f"
alias kdf="kubectl delete -f"
alias kdp="kubectl delete pod"
alias kdp="kubectl describe pod"
alias kdrs="kubectl describe replicasets.apps"
alias kds="kubectl describe service"
alias kgd="kubectl get deployments"
alias kgp="kubectl get pods"
alias kgpoj="kubectl get pods -o json"
alias kgpow="kubectl get pods -o wide"
alias kgpoy="kubectl get pods -o yaml"
alias ksys="kubectl --namespace=kube-system"
alias le="exa -la"
alias ohmyzsh="nvim ~/.oh-my-zsh"
alias rnd='task add +rnd +next'
alias rnr=read_and_review
alias sba="source .venv/bin/activate"
alias t="task"
alias task-due-week="task +WEEK list"
alias tfaa="terraform apply -auto-approve"
alias think='tickle +1d'
alias tick=tickle
alias tkill="for s in \$(tmux list-sessions | awk '{print \$1}' | rg ':' -r '' | fzy); do tmux kill-session -t \$s; done;"
alias v="nvim"
alias vim="nvim"
alias vssh="vagrant ssh"
alias zsh.config="nvim ~/.zshrc"
